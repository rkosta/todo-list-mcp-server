{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug setup-db in Docker",
      "type": "node",
      "request": "attach",
      "port": 9229,
      "address": "localhost",
      "localRoot": "${workspaceFolder}",
      "remoteRoot": "/app",
      "protocol": "inspector",
      "restart": false,
      "skipFiles": [
        "<node_internals>/**",
        "**/node_modules/**"
      ],
      "sourceMaps": true,
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ],
      "timeout": 30000,
      "stopOnEntry": false,
      "autoAttachChildProcesses": false,
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "trace": true
    },
